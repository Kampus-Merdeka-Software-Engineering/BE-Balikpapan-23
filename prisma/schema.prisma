// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Tracking {
  id Int @id @default(autoincrement())
  no_resi String
  tanggal DateTime 
  status String
}

model Tarif {
  id Int @id @default(autoincrement())
  origin String
  destination String 
  price Int
}

model Review {
  id Int @id @default(autoincrement())
  name String
  email String 
  subject String
  message String
  }
  